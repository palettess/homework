#include<stdio.h>
int main()
{
	float yongjing,value;
	
	printf("Enter value of trade:\n");
	scanf("%f",&value);
	
	if(value<2500.00f) yongjing=30.00f+.017f*value;
	else if(value<6250.00f) yongjing=56.00f+.0066f*value;
	else if(value<20000.00f) yongjing=76.00f+.0034f*value;
	else if(value<50000.00f) yongjing=100.00f+.0022f*value;
	else if(value<500000.00f) yongjing=155.00f+.0011f*value;
	else yongjing=255.00f+.0009f*value;
	
	if(yongjing<39.00f) yongjing=39.00f;
	
	printf("yongjing:$%.2f",yongjing);
	
	return 0;
}
